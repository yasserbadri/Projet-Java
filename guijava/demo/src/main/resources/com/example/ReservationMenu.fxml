<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="480.0" prefWidth="640.0" styleClass="button4" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ReservationMenuController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Label alignment="CENTER" prefHeight="8.0" prefWidth="568.0" style="-fx-font-size: 20px; -fx-font-weight: bold;" text="Gestion de Réservations" />
    <Button alignment="BASELINE_CENTER" contentDisplay="CENTER" onAction="#GoToafficher_reservations" prefHeight="0.0" prefWidth="607.0" text="Afficher les réservations" textAlignment="CENTER" />
    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#GoToajouter_reservation" text="Ajouter une réservation" textAlignment="CENTER" />
    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#GoTorechercher_reservation" text="Rechercher une réservation" textAlignment="CENTER" />
    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#GoTosupprimer_reservation" text="Supprimer une réservation" textAlignment="CENTER" />
    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#goBack" text="Retourner" textAlignment="CENTER" />
</VBox>
